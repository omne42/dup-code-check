{
  "name": "dup-code-check",
  "version": "0.0.1",
  "description": "Duplication checker (Rust binary; Node.js installer)",
  "author": "omne42",
  "keywords": [
    "duplication",
    "code",
    "scanner",
    "cli",
    "rust"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/omne42/dup-code-check.git"
  },
  "bugs": {
    "url": "https://github.com/omne42/dup-code-check/issues"
  },
  "homepage": "https://omne42.github.io/dup-code-check/",
  "bin": {
    "dup-code-check": "bin/dup-code-check.mjs"
  },
  "files": [
    "bin/",
    "crates/",
    "scripts/",
    "Cargo.lock",
    "Cargo.toml",
    "rust-toolchain.toml",
    "README.md",
    "README.zh-CN.md",
    "CHANGELOG.md",
    "CHANGELOG.zh-CN.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "node scripts/build-binary.mjs",
    "postinstall": "node scripts/postinstall.mjs",
    "prepack": "node scripts/prepack.mjs",
    "check": "node scripts/smoke.mjs",
    "test": "node scripts/smoke.mjs",
    "docs:llms": "node scripts/generate-llms.mjs",
    "docs:build": "npm run -s docs:llms && vitepress build docs",
    "docs:serve": "npm run -s docs:llms && vitepress dev docs"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "vitepress": "^1.6.3"
  },
  "engines": {
    "node": ">=22"
  }
}
